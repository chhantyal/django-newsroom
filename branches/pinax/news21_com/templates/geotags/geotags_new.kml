<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.1">
<Document>
{% for place in places %}
  <Placemark>
    <name>{{ place.object }}</name>
    <description>{{ place.object.description }}</description>
    <url>{{ place.object.permalink }}</url>
    {{ place.kml|safe }}
  </Placemark>{% endfor %}
</Document>
</kml>
